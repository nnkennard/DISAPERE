    {
      "review_id": "HJe5Adneor",
      "rebuttal_id": "r1efiwuroH",
      "sentence_index": 5,
      "text": "Similar to 1/7 experiments on the MNIST dataset, we let $p_{\\bar{d}}$ be the distribution of face images with glasses and without glasses, and let $p_{d}$ be images without glasses.",
      "suffix": "",
      "rebuttal_stance": "concur",
      "rebuttal_action": "rebuttal_answer",
